{
  "name": "eslint-config-appnexus",
  "version": "3.1.0",
  "description": "AppNexus Standard Eslint Config",
  "main": "index.js",
  "scripts": {
    "test": "jest -i --no-watchman",
    "watch": "jest --watch",
    "lint": "eslint . --ignore-pattern test-*",
    "lint-fix": "eslint . --ignore-pattern test-* --fix",
    "find-unused-rules": "eslint-find-rules -u -n",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check"
  },
  "pre-push": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/appnexus/eslint-config-appnexus"
  },
  "keywords": [
    "AppNexus",
    "config",
    "eslint",
    "eslintconfig",
    "javascript",
    "styleguide"
  ],
  "author": "Tim Santeford <tsanteford@appnexus.com>",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "dependencies": {
    "eslint-config-prettier": "^6.10.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-find-rules": "^3.4.0",
    "eslint-plugin-hapi": "^4.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-lodash": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^25.1.0",
    "lodash": "^4.17.15",
    "pre-push": "^0.1.1",
    "prettier": "^1.19.1",
    "react": "^16.13.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
